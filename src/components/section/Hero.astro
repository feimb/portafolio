---
import Section from "../Section.astro"
import { basics } from "@cv";

const { name, label, image } = basics
---

<Section>
    <div class="container">
        <div class="info">
            <h1>{name}</h1>
            <h2>{label}</h2>
            
            <figure>
                <img src={image} alt={name}>
            </figure>
        </div>
    </div>
</Section>

